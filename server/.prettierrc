{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "printWidth": 120,
  "useTabs": false,
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "parser": "typescript",
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "importOrder": ["^@nestjs/(.*)$', '<THIRD_PARTY_MODULES>', '^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}
